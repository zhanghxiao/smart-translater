{
  "name": "smart-translater",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start:server": "node -r dotenv/config server.js",
    "dev:server": "nodemon -r dotenv/config server.js",
    "start": "concurrently \"npm run serve\" \"npm run start:server\"",
    "dev": "concurrently \"npm run serve\" \"npm run dev:server\""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.8.3",
    "dotenv": "^16.4.5",
    "element-ui": "^2.15.13",
    "express": "^4.17.1",
    "markdown-it": "^13.0.1",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "nodemon": "^2.0.7",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
